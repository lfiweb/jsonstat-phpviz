{
  "name": "lfiweb/jsonstat-phpviz",
  "description": "Render JSON-stat data as multidimensional html tables",
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Simon Speich",
      "email": "simon.speich@wsl.ch"
    }
  ],
  "require": {
    "ext-dom": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpoffice/phpspreadsheet": "1.*",
    "phpunit/phpunit": "10.*"
  },
  "suggest": {
    "phpoffice/phpspreadsheet": "1.*; Install if you want to render/export JSON-stat as an Excel file"
  },
  "autoload": {
    "psr-4": {
      "jsonstatPhpViz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jsonstatPhpViz\\Test\\": "tests/phpunit/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}